    "@types/styled-components": "^5.1.24",
    "antd": "^4.19.5",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "4.0.3",
    "shortid": "^2.2.15",
